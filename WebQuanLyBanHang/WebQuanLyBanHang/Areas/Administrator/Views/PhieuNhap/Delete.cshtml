@model WebQuanLyBanHang.Model.PhieuNhap

@{
    ViewBag.Title = "Delete";
}

<h2>Xoá</h2>

<h3>Bạn muốn xoá mục này?</h3>
<div>
    <h4>PhieuNhap</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Mã phiếu nhập
        </dt>

        <dd>
            @Html.DisplayFor(model => model.MaCTPNhap)
        </dd>

        <dt>
            Mã nhập xuất
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ThongKeNhapXuat.MaNX)
        </dd>

        <dt>
            Tháng
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Thang)
        </dd>

        <dt>
            Mã sản phẩm
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SanPham.MaSP)
        </dd>

        <dt>
            Tên sản phẩm
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SanPham.TenSP)
        </dd>

        <dt>
            Số lượng nhập
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SLNhap)
        </dd>

        <dt>
            Giá nhập
        </dt>

        <dd>
            @Html.DisplayFor(model => model.GiaNhap)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Xoá" class="btn btn-default" /> |
            @Html.ActionLink("Quay lại", "Index")
        </div>
    }
</div>
