@model WebQuanLyBanHang.Model.ThongKeNhapXuat

@{
    ViewBag.Title = "Delete";
}

<h2>Xoá</h2>

<h3>Bạn muốn xoá mục này?</h3>
<div>
    <h4>Thống kê nhập xuất</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Mã báo cáo
        </dt>

        <dd>
            @Html.DisplayFor(model => model.BaoCaoThongKe.MaBC)
        </dd>

        <dt>
            Tháng
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Thang)
        </dd>

        <dt>
            Số tiền chi
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SoTienChi)
        </dd>

        <dt>
            Số tiền thu
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SoTienThu)
        </dd>

        <dt>
            Số tiền lãi
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SoTienLai)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Xoá" class="btn btn-default" /> |
            @Html.ActionLink("Quay lại", "Index")
        </div>
    }
</div>
